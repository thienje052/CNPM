diff --git a/CNPM/src/main/webapp/css/main.css b/CNPM/src/main/webapp/css/main.css
new file mode 100644
--- /dev/null
+++ b/CNPM/src/main/webapp/css/main.css
@@ -0,0 +1,210 @@
+
+/* === Reset & Body === */
+body {
+  font-family: Arial, sans-serif;
+  margin: 0;
+  padding: 0;
+  background: #fff;
+}
+
+/* === Header === */
+.header, header {
+  background-color: #e0e0e0;
+  text-align: center;
+  padding: 10px;
+  font-size: 25px;
+  font-weight: bold;
+  color: #0D47A1;
+}
+
+/* === Layout Section === */
+.section, .form-container, .main-content {
+  padding: 20px;
+  margin: 0 auto;
+  display: flex;
+  flex-wrap: wrap;
+  gap: 20px;
+}
+
+/* === Form === */
+form, .form-inline, .form-group, .form-row, .form-top {
+  display: flex;
+  flex-wrap: wrap;
+  gap: 20px;
+  margin-bottom: 15px;
+}
+
+.form-group {
+  flex-direction: column;
+  width: 250px;
+}
+
+.form-group label {
+  font-size: 14px;
+  margin-bottom: 5px;
+}
+
+.form-group input,
+.form-group select,
+select,
+input[type="radio"] {
+  padding: 8px;
+  border: 1px solid #ccc;
+  border-radius: 6px;
+  width: 100%;
+  box-sizing: border-box;
+}
+
+input[type="radio"] {
+  transform: scale(1.2);
+}
+
+/* === Buttons === */
+.btn-submit, .action-buttons button, .form-button button, .form-buttons button, .footer-buttons button, .nut button {
+  padding: 10px 20px;
+  background-color: #007bff;
+  color: white;
+  border: none;
+  border-radius: 6px;
+  cursor: pointer;
+}
+
+.btn-submit:hover,
+.action-buttons button:hover,
+.form-button button:hover,
+.form-buttons button:hover,
+.footer-buttons button:hover,
+.nut button:hover {
+  background-color: #0056b3;
+}
+
+/* === Table === */
+table {
+  width: 100%;
+  border-collapse: collapse;
+  margin-top: 10px;
+  font-size: 14px;
+}
+
+table, th, td {
+  border: 1px solid #ccc;
+  text-align: center;
+  padding: 8px;
+}
+
+th {
+  background-color: #f1f1f1;
+}
+
+/* === Others === */
+.container {
+  width: 90%;
+  margin: 0 auto;
+  padding: 20px 0;
+}
+
+.box, .nut {
+  border: 1px solid black;
+  border-radius: 8px;
+  padding: 16px;
+  width: 200px;
+  text-align: center;
+  box-sizing: border-box;
+  margin: 10px;
+}
+
+.section-title {
+  font-weight: bold;
+  border-bottom: 1px dashed #000000;
+  margin-bottom: 5px;
+  padding-bottom: 4px;
+}
+
+.section-number {
+  font-size: 18px;
+}
+
+.summary {
+  margin-top: 10px;
+}
+
+
+/* === Account and Doitac Specific Styles === */
+
+.container {
+  padding: 20px 40px;
+}
+
+.form-column {
+  flex: 1;
+  min-width: 300px;
+}
+
+.form-group {
+  display: flex;
+  flex-direction: column;
+  margin-bottom: 15px;
+}
+
+label {
+  margin-bottom: 5px;
+  font-size: 14px;
+}
+
+.checkbox-group {
+  margin-top: 8px;
+}
+
+.checkbox-group label {
+  display: block;
+  font-size: 14px;
+  margin: 3px 0;
+}
+
+.required {
+  color: red;
+}
+
+.button-group {
+  display: flex;
+  gap: 10px;
+  margin-top: 20px;
+  text-align: center;
+  align-items: flex-end;
+}
+
+.button-group button,
+a.button,
+button.button {
+  padding: 8px 20px;
+  margin: 0 10px;
+  border: none;
+  border-radius: 4px;
+  color: white;
+  font-size: 14px;
+  cursor: pointer;
+  background-color: #007bff;
+  text-decoration: none;
+}
+
+.button-group .button-cancel {
+  background-color: #6c757d;
+}
+
+a.button:hover,
+button.button:hover,
+.button-group button:hover {
+  background-color: #0056b3;
+}
+
+/* === Doitac === */
+.btn-custom {
+  width: 90px;
+  padding: 8px 0;
+}
+
+.table td, .table th {
+  vertical-align: middle;
+  text-align: center;
+}
